#! /usr/bin/env python

import sys, os
string=""
for i in sys.argv:
	if i!=sys.argv[0]:
		string=string+" "+i
print "---&START&---"
os.system("""sudo -p "Please enter your password to run serpint: (the default password for the account pi is 'raspberry') " python serpint.py"""+string)
print "---&EXIT*&---"
